<% for(var i =0 ; i<user.length ; i++){%>
<tr>
    <td><%= i+1%></td>
    <td><%= user[i].name%></td>
    <td><%= user[i].email%></td>
    <td><%= user[i].gender%></td>
    <td><%= user[i].status%></td>
    <td>
        <a href="/update-user?id=<%= user[i]._id%>"class="btn border-shadow update">
            <span class="text-gradient" ><i class="fa-solid fa-pencil"></i></span>
        </a>

        <a href="#"class="btn border-shadow delete" data-id="<%= user[i]._id%>">
            <span class="text-gradient" ><i class="fa-solid fa-trash"></i></span>
        </a>
        
    </td>

</tr>

<%} %> 